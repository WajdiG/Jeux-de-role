prog4: IA.o pile_tab.o region.o accueil.o deplacement.o
	gcc IA.o pile_tab.o region.o accueil.o deplacement.o -o prog4
	
pile_tab.o : pile_tab.c
	gcc -c pile_tab.c
	
region.o : region.c
	gcc -c region.c
	
IA.o: IA.c 
	gcc -c IA.c 
	
accueil.o : accueil.c
	gcc -c accueil.c
	
deplacement.o : deplacement.c
	gcc -c deplacement.c 

clean: 
	rm -f *.o prog4
